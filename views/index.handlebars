<div id="videos-container" class="contents-container">
  {{#each videos as |video|}}
    <div class="video-card">
      <iframe class="video-player" frameborder="0" src="{{video.url}}"></iframe>
      <div class="video-title">
        <a href="/videos/{{video._id}}">
          {{video.title}}
        </a>
      </div>
    </div>
  {{/each}}
</div>
<a href="/videos/create" class="add-video-button">
  Add a video
</a>
